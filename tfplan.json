+ resource "rafay_eks_cluster" "ekscluster-advanced" {
  + id = (known after apply)

  + cluster {
      + kind = "Cluster"

      + metadata {
          + name    = "abhinav-clustercraft-eks-gitops"
          + project = "podify"
        }

      + spec {
          + blueprint         = "default"
          + blueprint_version = "Latest"
          + cloud_provider    = "abhinav1rafayeks"
          + cni_provider      = "aws-cni"
          + type              = "eks"
        }
    }

  + cluster_config {
      + apiversion = "rafay.io/v1alpha5"
      + kind       = "ClusterConfig"

      + managed_nodegroups {
          + ami_family         = "AmazonLinux2"
          + desired_capacity   = 1
          + disable_imdsv1     = false
          + disable_pods_imds  = false
          + efa_enabled        = false
          + instance_type      = "t3.xlarge"
          + max_pods_per_node  = 50
          + max_size           = 2
          + min_size           = 1
          + name               = "managed-ng-1"
          + private_networking = true
          + spot               = false
          + tags               = {
              + "email" = "abhinav@rafay.co"
              + "env"   = "cs-team"
            }
          + version            = "1.28"
          + volume_iops        = 3000
          + volume_size        = 80
          + volume_throughput  = 125
          + volume_type        = "gp3"
        }

      + metadata {
          + name    = "abhinav-clustercraft-eks-gitops"
          + region  = "us-west-2"
          + tags    = {
              + "demo"  = "true"
              + "email" = "abhinav@rafay.co"
              + "env"   = "cs-team"
            }
          + version = "1.28"
        }

      + vpc {
          + cluster_endpoints {
              + private_access = true
              + public_access  = false
            }
        }
    }
}